{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [4, "space"],
        "quotes": ["error", "single", { "avoidEscape": true, "allowTemplateLiterals": true }]
    },
    "plugins": [
        "import"
    ]
}
