  "dependencies": {
    "bulma": "^0.5.3"
  },
  "devDependencies": {
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.1",
